{
  "name": "react-forms-ui",
  "version": "0.7.0",
  "description": "A comprehensive UI toolbox for React.",
  "main": "lib/index",
  "scripts": {
    "build": "rimraf lib && babel ./src -d lib && css-combine src/react-forms-ui.css > lib/react-forms-ui.css",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ivos/react-forms-ui.git"
  },
  "keywords": [
    "React",
    "form",
    "forms",
    "UI",
    "Bootstrap"
  ],
  "author": "Ivo Maixner <ivo.maixner@gmail.com> (http://lastware.cz)",
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/ivos/react-forms-ui/issues"
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "font-awesome": "^4.6.3",
    "i18next": "^3.4.1",
    "jquery": "^2.2.4",
    "moment": "^2.14.1",
    "numeral": "^1.5.3",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.3.1",
    "select2": "^3.5.1"
  },
  "devDependencies": {
    "babel-cli": "~6.4.0",
    "babel-core": "~6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.4.0",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.3.13",
    "css-combine": "^1.0.0",
    "rimraf": "~2.5.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
}
